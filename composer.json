{
    "name": "steinbauerit/cookieoptin",
    "type": "typo3-cms-extension",
    "description": "A TYPO3 CMS Extension for Cookie Opt-in.",
    "authors": [
        {
            "name": "Patric Eckhart",
            "role": "Developer"
        }
    ],
    "require": {
        "typo3/cms-core": "^8.7 || ^9.5 || ^10.4"
    },
    "autoload": {
        "psr-4": {
            "SteinbauerIT\\Cookieoptin\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SteinbauerIT\\Cookieoptin\\Tests\\": "Tests"
        }
    },
    "replace": {
        "steinbauerit/cookieoptin": "self.version",
        "typo3-ter/cookieoptin": "self.version"
    }
}